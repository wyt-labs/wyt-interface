{
  "private": true,
  "scripts": {
    "start": "cross-env TAILWIND_MODE=watch NODE_OPTIONS=--openssl-legacy-provider umi dev",
    "build": "NODE_OPTIONS=--openssl-legacy-provider umi build && tsc --build tsconfig.api.json",
    "prebuild": "NODE_OPTIONS=--openssl-legacy-provider umi build",
    "postinstall": "umi generate tmp",
    "test": "umi-test",
